      <!-- <pose>0.25 0 0.25 0 0 0</pose> -->
      <collision name="collision">
        <geometry>
          <box>
            <size> 0.5 0.5 0.5</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <cast_shadows>false</cast_shadows>
        <geometry>
          <box>
            <size> 0.5 0.5 0.5</size>
          </box>
        </geometry>
        <material>
          <emissive>1 1 1 0</emissive>
        </material>
      </visual>
    </link>
    <joint name="joint" type="fixed">
      <pose></pose>
      <parent link="box"/>
      <child link="tag"/>
    </joint>
    <plugin name="tag_box_motion" filename="libtag_box_plugin.so"></plugin>
  </model>
