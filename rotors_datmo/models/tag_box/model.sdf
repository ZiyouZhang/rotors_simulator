<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="tag_box">
    <static>true</static>
    <link name="tag">
      <!-- <pose>0 0 0.25 0 0 0</pose> -->
      <!-- <collision name="collision">
        <geometry>
          <box>
            <size> 1 0.5 0.5</size>
          </box>
        </geometry>
      </collision> -->
      <visual name="visual">
        <cast_shadows>false</cast_shadows>
        <geometry>
          <box>
            <size> 0.001 0.5 0.5</size>
          </box>
        </geometry>
        <material>
          <emissive>1 1 1 1</emissive>
          <script>
            <uri>model://tag_box/materials/scripts</uri>
            <uri>model://tag_box/materials/textures</uri>
            <name>TagBox</name>
          </script>
        </material>
      </visual>
